{
  "manifest_version": 2,

  "name": "Bagelbot Cart Resolver",
  "description": "Resolve cart guid's into actual balsam bagel carts",
  "version": "1.0.0",

  "permissions": [
    "tabs",
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["https://www.toasttab.com/balsam-bagels/v3/*"],
      "css": [],
      "js": ["content-script.js"]
    }
  ]
}
